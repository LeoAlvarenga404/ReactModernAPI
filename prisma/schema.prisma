generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Estudo {
  id                  Int      @id @default(autoincrement())
  nome                String
  descricao           String
  porcentagemConcluida Int
  finalizado          Boolean
  createdAt           DateTime @default(now())
  updatedAt           DateTime @updatedAt
}
